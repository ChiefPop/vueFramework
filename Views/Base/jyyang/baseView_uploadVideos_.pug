div
  el-upload("class"="upload-demo"
            "action"="https://jsonplaceholder.typicode.com/posts/"
            ":on-preview"="handlePreview"
            ":on-remove"="handleRemove"
            ":before-remove"="beforeRemove"
            ":http-request"="uploadVidos"
            ":accept"="'.wmv,.avi,.dat,.rm,.rmvb,.mp4,.mov'"
            ":file-list"="videoList")
    el-button("size"="small" "type"="primary") Click to upload

  el-dialog("title"="File uploading" ":visible.sync"="dialogVisible" "width"="30%" ":before-close"="handleDialogClose")
    el-progress(":percentage"="uploadPercentage" ":status"="uploadStatus")

  el-dialog("title"="Video management" ":visible.sync"="videoMangVisible" "width"="30%")